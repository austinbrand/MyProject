                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>
        }

        else
        {
            <tr>
                <td>
                    @Html.DisplayText("Error happened with user information or view.")
                </td>
            </tr>
        }


    }
